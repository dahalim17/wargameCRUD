	<!-- DATA JSON -->
	<script src="jquery-3.4.1.js"></script>
	<script>
		$.getJSON("data/games.json", function (data) {
			var items = [];
			$.each(data, function (key, val) {
				items.push("<tr>");
				items.push("<td id='" + key + "'>" + val.id + "</td>");
				items.push("<td id='" + key + "'>" + val.games + "</td>");
				items.push("<td id='" + key + "'>" + val.genre + "</td>");
				items.push("<td id='" + key + "'>" + val.price + "</td>");
				items.push("<td id='" + key + "'>" + val.date + "</td>");
				items.push("</tr>");
			});
			$("<tbody/>", {
				"class": "mydata",
				html: items.join("")
			}).appendTo("table");
		});
	</script>
